---
const redirectUrl = "https://www.apple.com/legal/internet-services/itunes/dev/stdeula/";
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Terms of Service - Alimor</title>
    <meta name="description" content="Alimor Terms of Service - Apple Standard EULA" />
    <meta http-equiv="refresh" content={`0; url=${redirectUrl}`} />
    <link rel="canonical" href={redirectUrl} />
  </head>
  <body>
    <script define:vars={{ redirectUrl }}>
      window.location.replace(redirectUrl);
    </script>
    <p>Redirecting to <a href={redirectUrl}>Terms of Service</a>...</p>
  </body>
</html>
